{
  "name": "gbf-tracker",
  "version": "0.1.0",
  "description": "Simple Granblue Fantasy collection tracker",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d eval --mode=\"development\" && copy src\\index.html dist\\index.html && webpack serve --mode=\"development\"",
    "build:prod": "webpack -p --mode=\"production\" && copy src/index.html dist/index.html"
  },
  "keywords": [
    "gbf",
    "reactjs"
  ],
  "author": "Rui Santos <ruimcs20@gmail.com>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
